<h1>Usu√°rios cadastrados</h1>
<div class="container">
  {{#each users}}
    <div class="item">
      <h2>{{this.name}}</h2>
      <p>{{this.occupation}}</p>
      <div class="actions">
        <a href="/users/{{this.id}}" class="details-btn">See details</a>
        <form action="/users/delete/{{this.id}}" method="POST">
          <input type="submit" value="Remove" class="delete-btn">
        </form>
    </div>
  {{/each}}

</div>